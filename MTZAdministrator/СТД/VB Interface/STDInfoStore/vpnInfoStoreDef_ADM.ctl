Private Sub opInfoStoreDef_STDInfoStore_SetName_ADM(RowItem As Object)
With RowItem
Run_VBUpdateItemName Name:=.brief, RowItem:=RowItem
End With
End Sub
Private Sub cmdInfoStoreDefRef_Click()
  On Error Resume Next
  Item.InfoStoreDef.Refresh
  cmdInfoStoreDefAdd_Click
End Sub
Private Sub cmdInfoStoreDefCfg_Click()
    On Error Resume Next
    editInfoStoreDef.Customize
    Dim ff As Long
    ff = FreeFile
    Open GetSetting("MTZ", "CONFIG", "LAYOUTS", "c:\") & "STDInfoStore_editInfoStoreDef_ADM" For Output As #ff
    Print #ff, editInfoStoreDef.PanelCustomisationString
    Close #ff
End Sub
Private Sub cmdInfoStoreDefAdd_Click()
  On Error Resume Next
 Dim u As Object
 If Item.InfoStoreDef.Count = 0 Then
   Set u = Item.InfoStoreDef.Add

 End If
 Set editInfoStoreDef.Item = Item.InfoStoreDef.Item(1)
 editInfoStoreDef.InitPanel
 cmdInfoStoreDefSave.Enabled = True
End Sub
Private Sub cmdInfoStoreDefDel_Click()
  On Error Resume Next
  If editInfoStoreDef.Item Is Nothing Then Exit Sub
  If MsgBox("Удалить элемент" & vbCrLf & editInfoStoreDef.Item.brief & vbCrLf & "?", vbYesNo + vbQuestion, "Внимание") = vbYes Then
    On Error GoTo bye
    editInfoStoreDef.Item.Parent.Delete editInfoStoreDef.Item.ID
    cmdInfoStoreDefSave.Enabled = False
    editInfoStoreDef.Enabled = False
    Set editInfoStoreDef.Item = Nothing
  End If
Exit Sub
bye:
MsgBox err.Description, vbOKOnly + vbExclamation, "Удаление"
End Sub
Private Sub editInfoStoreDef_Changed()
 cmdInfoStoreDefSave.Enabled = True
End Sub
Public Function IsOK() As Boolean
    If editInfoStoreDef.IsOK Then
        IsOK = True
    End If
End Function
Private Sub cmdInfoStoreDefSave_Click()
  On Error Resume Next
    If editInfoStoreDef.IsOK Then
    MousePointer = vbHourglass
    editInfoStoreDef.Save
  On Error GoTo bye
    editInfoStoreDef.Item.Save
    MousePointer = vbNormal
        opInfoStoreDef_STDInfoStore_SetName_ADM editInfoStoreDef.Item
    cmdInfoStoreDefSave.Enabled = False
    Else
      MsgBox "Не все обязательные поля заполнены", vbOKOnly + vbExclamation
    End If
Exit Sub
bye:
    MousePointer = vbNormal
   MsgBox err.Description, vbOKOnly + vbExclamation, "Изменение"
  editInfoStoreDef.Item.Refresh
  editInfoStoreDef.InitPanel
End Sub

'Функция инициализации панели данными
'Parameters:
'[IN][OUT]  aItem , тип параметра: object,
'[IN][OUT]   optional pForm , тип параметра: object = nothing  - ...
'See Also:
'Example:
'  call me.OnInit({параметры})
Public Sub OnInit(aItem As Object, Optional pForm As Object = Nothing)
 On Error Resume Next
 Set Item = aItem
 Set ParentForm = pForm

  LoadBtnPictures cmdInfoStoreDefSave, cmdInfoStoreDefSave.Tag
  LoadBtnPictures cmdInfoStoreDefRef, cmdInfoStoreDefRef.Tag
  LoadBtnPictures cmdInfoStoreDefCfg, cmdInfoStoreDefCfg.Tag
  Dim ff As Long, buf As String
  ff = FreeFile
  On Error Resume Next
  Open GetSetting("MTZ", "CONFIG", "LAYOUTS", App.Path & "\LAYOUTS\") & "STDInfoStore_editInfoStoreDef_ADM" For Input As #ff
  buf = ""
  buf = Input(LOF(ff), ff)
  Close #ff
  If buf <> "" Then editInfoStoreDef.PanelCustomisationString = buf
  cmdInfoStoreDefAdd_Click
End Sub

