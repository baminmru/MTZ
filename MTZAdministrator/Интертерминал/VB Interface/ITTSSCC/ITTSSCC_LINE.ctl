'Контроль правильности заполнения данных на панели редактирования
'Parameters:
' параметров нет
'Returns:
' Boolean, семантика результата:
'   true  -
'   false -
'See Also:
'Example:
' dim variable as boolean
'  variable = me.IsOK()
Public Function IsOK() As Boolean
  On Error Resume Next
  Dim mIsOK As Boolean
  mIsOK = True

If mIsOK Then mIsOK = IsSet(txtPalletCode.Text)
If mIsOK Then mIsOK = IsSet(txtBarcode1.Text)
If mIsOK Then mIsOK = (cmbProcessed.ListIndex >= 0)
  IsOK = mIsOK
End Function
Private Function AddSQLRefIds(ByVal strTo As String, ByVal fldName As String, ByVal strFrom As String) As String
  Dim XMLDocFrom As New DOMDocument
  Dim XMLDocTo As New DOMDocument
  AddSQLRefIds = strTo
  On Error GoTo err
  Call XMLDocTo.loadXML(strTo)
  Call XMLDocFrom.loadXML(strFrom)
  Dim Node As MSXML2.IXMLDOMNode
  Dim ID As String
  For Each Node In XMLDocFrom.childNodes.Item(0).childNodes
    If (Node.baseName = "ID") Then
      ID = Node.Text
      Dim NodeTO As MSXML2.IXMLDOMNode
      Dim bAdded As Boolean
      bAdded = False
      For Each NodeTO In XMLDocTo.childNodes.Item(0).childNodes
       If (NodeTO.baseName = fldName & "ID") Then
         NodeTO.Text = ID
         bAdded = True
         Exit For
       End If
      Next
      If (Not bAdded) Then
       Dim newNode As MSXML2.IXMLDOMNode
       Set newNode = XMLDocTo.createNode(MSXML2.NODE_ELEMENT, fldName & "ID", XMLDocTo.namespaceURI)
        newNode.Text = ID
       Call XMLDocTo.childNodes.Item(0).appendChild(newNode)
      End If
      AddSQLRefIds = XMLDocTo.xml
      Exit For
    End If
  Next
err:
End Function

'Инициализация контролов панели
'Parameters:
' параметров нет
'See Also:
'Example:
'  call me.InitPanel()
Public Sub InitPanel()
OnInit = True
Dim iii As Long ' for combo only
If Item.CanChange Then
  Panel.Enabled = True
Else
  Panel.Enabled = False
End If

  On Error Resume Next
txtPalletCode = Item.PalletCode
  On Error Resume Next
txtBarcode1 = Item.Barcode1
  On Error Resume Next
txtBarcode2 = Item.Barcode2
cmbProcessed.Clear
cmbProcessed.AddItem "Да"
cmbProcessed.ItemData(cmbProcessed.NewIndex) = -1
cmbProcessed.AddItem "Нет"
cmbProcessed.ItemData(cmbProcessed.NewIndex) = 0
 For iii = 0 To cmbProcessed.ListCount - 1
  If Item.Processed = cmbProcessed.ItemData(iii) Then
   cmbProcessed.ListIndex = iii
   Exit For
  End If
 Next
txtResult = Item.result
' Добавим значения ID ко всем SQLReference
OnInit = False
End Sub
Private Sub Changing()
If OnInit Then Exit Sub

 mIsChanged = True
 RaiseEvent Changed
End Sub

'Сохранение
'Parameters:
' параметров нет
'See Also:
'Example:
'  call me.Save({параметры})
Public Sub Save()
If OnInit Then Exit Sub

Item.PalletCode = txtPalletCode
Item.Barcode1 = txtBarcode1
Item.Barcode2 = txtBarcode2
 If cmbProcessed.ListIndex >= 0 Then
   Item.Processed = cmbProcessed.ItemData(cmbProcessed.ListIndex)
 End If
Item.result = txtResult
 mIsChanged = False
 RaiseEvent Changed
End Sub

