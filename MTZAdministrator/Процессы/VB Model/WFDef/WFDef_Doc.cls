'Упаковать поля в именованную коллекцию
'Parameters:
'[IN][OUT]   nv , тип параметра: NamedValues  - ...
'See Also:
'Example:
'  call me.Pack({параметры})
Public Sub Pack(ByRef nv As NamedValues)
  Dim nvi As NamedValue
  Set nv = Nothing
  Set nv = New NamedValues

  Set nvi = nv.Add("Name", m_Name)
nvi.ValueSize = 255
  Set nvi = nv.Add("SetBeforStart", m_SetBeforStart)
nvi.ValueType = adInteger
  Set nvi = nv.Add("CreateInRuntime", m_CreateInRuntime)
nvi.ValueType = adInteger
 ' Reference to object
    If m_DocTemplate_ID = "" Then
       Set nvi = nv.Add("DocTemplate", "")
    Else
       Set nvi = nv.Add("DocTemplate", m_DocTemplate_ID)
    End If
    nvi.ORACLE_GUID
 ' Reference to Folder
    If m_ArchveFolder_ID = "" Then
       Set nvi = nv.Add("ArchveFolder", "")
    Else
       Set nvi = nv.Add("ArchveFolder", m_ArchveFolder_ID)
    End If
    nvi.ORACLE_GUID
  Set nvi = nv.Add("ArchiveMode", m_ArchiveMode)
nvi.ValueSize = 255
 ' Reference to OBJECTTYPE
    If m_DocType_ID = "" Then
       Set nvi = nv.Add("DocType", "")
    Else
       Set nvi = nv.Add("DocType", m_DocType_ID)
    End If
    nvi.ORACLE_GUID
End Sub

'Краткое название строки
'Parameters:
'[IN]   UseServer , тип параметра: boolean = false  - ...
'Returns:
'  значение типа string
'See Also:
'Example:
' dim variable as string
' variable = me. Brief(...параметры...)
Public Function Brief(Optional ByVal UseServer As Boolean = False) As String
 Dim s As String
 If Application.WorkOffline Then

  m_Brief = ""
 m_Brief = m_Brief & Name & "; "
 Brief = m_Brief
 Else
  On Error GoTo bye
   If UseServer Or m_Brief = "" Then
     Application.MTZSession.GetBrief PartName, id, s
     If Len(s) > 255 Then
       Brief = Left(s, 255) & "..."
     Else
       Brief = s
     End If
       m_Brief = Brief
   Else
     Brief = m_Brief
   End If
 End If
Exit Function
bye:
    Brief = "! доступ запрещен ! (" & Err.Description & ")"
End Function

'Распаковать значения полей из именованной коллекции
'Parameters:
'[IN][OUT]   nv , тип параметра: object  - ...
'See Also:
'Example:
'  call me.UnPack({параметры})
Public Sub UnPack(ByRef nv As Object)
 On Error Resume Next
If TypeName(nv) = "NamedValues" Then

m_Name = nv.item("Name").Value
m_SetBeforStart = nv.item("SetBeforStart").Value
m_CreateInRuntime = nv.item("CreateInRuntime").Value
  m_DocTemplate_ID = nv.item("DocTemplate").Value
  m_ArchveFolder_ID = nv.item("ArchveFolder").Value
m_ArchiveMode = nv.item("ArchiveMode").Value
  m_DocType_ID = nv.item("DocType").Value
Else

m_Name = nv.Fields("Name").Value
m_SetBeforStart = nv.Fields("SetBeforStart").Value
m_CreateInRuntime = nv.Fields("CreateInRuntime").Value
  m_DocTemplate_ID = nv.Fields("DocTemplate").Value
  m_ArchveFolder_ID = nv.Fields("ArchveFolder").Value
m_ArchiveMode = nv.Fields("ArchiveMode").Value
  m_DocType_ID = nv.Fields("DocType").Value
End If
End Sub

