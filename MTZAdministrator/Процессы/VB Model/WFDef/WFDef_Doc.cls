'Загрузить из формата XML
'Parameters:
'[IN][OUT]   node , тип параметра: IXMLDOMNode,
'[IN]   LoadMode , тип параметра: integer=0  - ...
'See Also:
'Example:
'  call me.XMLLoad({параметры})
Public Sub XMLLoad(node As IXMLDOMNode, Optional ByVal LoadMode As Integer = 0)
  On Error Resume Next
  Dim e_list As IXMLDOMNodeList
  Dim e_ As IXMLDOMNode
  If LoadMode <> 2 Then m_ID = node.Attributes.getNamedItem("ID").nodeValue
  m_Deleted = node.Attributes.getNamedItem("Deleted").nodeValue
  m_IsLocked = node.Attributes.getNamedItem("IsLocked").nodeValue
  If m_IsLocked > 2 Then m_IsLocked = 0
  m_RetriveTime = node.Attributes.getNamedItem("RetriveTime").nodeValue
  m_ChangeTime = node.Attributes.getNamedItem("ChangeTime").nodeValue
  m_AccessTime = node.Attributes.getNamedItem("AccessTime").nodeValue
  If Application.WorkOffline And m_IsLocked = LockSession Then m_IsLocked = NoLock
  Secure node.Attributes.getNamedItem("SECURITYSTYLEID").nodeValue
  Name = Replace(node.Attributes.getNamedItem("NAME").nodeValue, vbLf, vbCrLf)
  SetBeforStart = node.Attributes.getNamedItem("SETBEFORSTART").nodeValue
  CreateInRuntime = node.Attributes.getNamedItem("CREATEINRUNTIME").nodeValue
  Set DocTemplate = Nothing
  m_DocTemplate_ID = node.Attributes.getNamedItem("DOCTEMPLATE").nodeValue
  m_DocTemplate_ID_BRIEF = node.Attributes.getNamedItem("DOCTEMPLATE_BRIEF").nodeValue
  Set ArchveFolder = Nothing
  m_ArchveFolder_ID = node.Attributes.getNamedItem("ARCHVEFOLDER").nodeValue
  m_ArchveFolder_ID_BRIEF = node.Attributes.getNamedItem("ARCHVEFOLDER_BRIEF").nodeValue
  ArchiveMode = Replace(node.Attributes.getNamedItem("ARCHIVEMODE").nodeValue, vbLf, vbCrLf)
  Set DocType = Nothing
  m_DocType_ID = node.Attributes.getNamedItem("DOCTYPE").nodeValue
  m_DocType_ID_BRIEF = node.Attributes.getNamedItem("DOCTYPE_BRIEF").nodeValue
  On Error GoTo bye
  'if m_ChangeTime >= m_RetriveTime then m_Changed = true
  m_Changed = True
  m_RowRetrived = True
  m_Brief = ""
  On Error Resume Next
  Exit Sub
bye:
  DebugOutput Err.Description
End Sub


'Сохранить в формате XML
'Parameters:
'[IN][OUT]  node , тип параметра: IXMLDOMElement,
'[IN][OUT]   xdom , тип параметра: DomDocument  - ...
'See Also:
'Example:
'  call me.XMLSave({параметры})
Public Sub XMLSave(node As IXMLDOMElement, xdom As DOMDocument)
On Error Resume Next
  Dim e_ As IXMLDOMElement
  LoadFromDatabase
  node.setAttribute "ID", m_ID
  node.setAttribute "Deleted", m_Deleted
  node.setAttribute "IsLocked", m_IsLocked
  node.setAttribute "RetriveTime", CDbl(m_RetriveTime)
  node.setAttribute "ChangeTime", CDbl(m_ChangeTime)
  node.setAttribute "SECURITYSTYLEID", m_SecureStyleID
  node.setAttribute "NAME", Name
  node.setAttribute "SETBEFORSTART", SetBeforStart
  node.setAttribute "CREATEINRUNTIME", CreateInRuntime
  node.setAttribute "DOCTEMPLATE", m_DocTemplate_ID
  If Not (DocTemplate Is Nothing) Then
    node.setAttribute "DOCTEMPLATE_BRIEF", DocTemplate.Brief
  Else
    node.setAttribute "DOCTEMPLATE_BRIEF", m_DocTemplate_ID_BRIEF
  End If
  node.setAttribute "ARCHVEFOLDER", m_ArchveFolder_ID
  If Not (ArchveFolder Is Nothing) Then
    node.setAttribute "ARCHVEFOLDER_BRIEF", ArchveFolder.Brief
  Else
    node.setAttribute "ARCHVEFOLDER_BRIEF", m_ArchveFolder_ID_BRIEF
  End If
  node.setAttribute "ARCHIVEMODE", ArchiveMode
  node.setAttribute "DOCTYPE", m_DocType_ID
  If Not (DocType Is Nothing) Then
    node.setAttribute "DOCTYPE_BRIEF", DocType.Brief
  Else
    node.setAttribute "DOCTYPE_BRIEF", m_DocType_ID_BRIEF
  End If
  node.setAttribute "AccessTime", CDbl(m_AccessTime)
End Sub

